{
    "name": "@tokololo/react-json-ptr-store",
    "version": "2.0.2",
    "description": "A json pointer and rxjs enabled reactive store for React",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "yarn build:esm && yarn build:cjs",
        "build:esm": "tsc --module esnext --outDir dist/esm",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
        "prepare": "npm run build",
        "test": "jest --config jestconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tokololo/react-json-ptr-store.git"
    },
    "keywords": [
        "typescript",
        "react",
        "json-pointer",
        "json-ptr",
        "rxjs",
        "store",
        "state-management"
    ],
    "author": "WillowDigit",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Tokololo/react-json-ptr-store/issues"
    },
    "homepage": "https://github.com/Tokololo/react-json-ptr-store#readme",
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/react": "^14.2.1",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.14.202",
        "@types/react": "^18.2.51",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@tokololo/json-ptr-store": "^1.1.3",
        "lodash": "^4.17.21",
        "rxjs": "^7.8.1"
    }
}
